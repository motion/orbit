{
  "name": "@mcro/ocr",
  "private": true,
  "version": "1.5.1",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "start": "node index.js",
    "start-prod": "NODE_ENV=production IS_PROD=true node lib/index.js",
    "start-dev": "nodemon --inspect=0.0.0.0:5858 lib/index.js",
    "build": "CHUNK_COMMONS=false MINIFY=true NODE_ENV=production BUILD_DIR=. ENTRY=./src mcro-build",
    "watch": "npm run build -- --watch",
    "test": "node lib/test.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.map",
      "lib",
      "dist"
    ]
  },
  "dependencies": {
    "@mcro/screendiff": "*",
    "cheerio": "^1.0.0-rc.2",
    "child_process": "^1.0.2",
    "google-nlp": "^0.5.6",
    "jimp": "^0.2.28",
    "path": "^0.12.7",
    "sb-promisify": "^2.0.2",
    "screenshot-node": "^1.2.1"
  },
  "devDependencies": {
    "@mcro/builder": "^1.5.1",
    "nodemon": "^1.11.0"
  }
}
