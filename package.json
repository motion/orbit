    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-jest": "22.3.2",
    "typescript-eslint-parser": "21.0.2",
    "lerna": "3.13.0",
    "mobx": "5.9.0",
    "prettier": "1.15.3",
    "sb-promisify": "^2.0.2",
