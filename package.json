{
  "name": "motion-starter",
  "private": true,
  "scripts": {
    "bootstrap": "node_modules/.bin/lerna bootstrap",
    "web": "cd apps/web && npm start",
    "clean": "rm -rf apps/*/lib && rm -rf apps/*/node_modules",
    "build": "lerna exec --ignore web -- babel src --out-dir lib",
    "compile": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "postinstall": "./build.sh"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-steelbrain": "^4.0.2",
    "flow-bin": "^0.38.0",
    "lerna": "^2.0.0-rc.2"
  }
}
