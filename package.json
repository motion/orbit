{
  "name": "orbit-monorepo",
  "private": true,
  "workspaces": [
    "app/*",
    "packages/*",
    "apps/*",
    "projects/*"
  ],
  "dependencies": {
    "global": "^4.3.2",
    "react": "16.8.3",
    "react-dom": "16.8.3"
  },
