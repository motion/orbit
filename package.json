    "ts-jest": "24.0.0"
  },
  "scripts": {
    "watch": "source .env && build --watch",
    "clean": "source .env && clean",
    "clean-packages": "source .env && clean -all",
    "start": "cd app/orbit && npm start",
    "start-prod": "cd app/orbit && npm run start-prod",
    "build-app": "cd app/build-orbit && npm run build-app",
    "build-app-resume": "cd app/build-orbit && npm run build-app-resume",
    "publish-app": "cd app/build-orbit && npm run publish",
    "publish-packages": "bin/publish",
    "test-built-app": "cd app/build-orbit && npm run test-built-app"
  }
}
