{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "html"
  ],
  "rules": {
    "semi": [2, "never"],
    "strict": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": [2, "never"],
    "func-names": 0,
    "no-param-reassign": 0,
    "comma-dangle": [2, "always-multiline"],
    "quotes": [0],
    "prefer-rest-params": 0,
    "max-len": [1, 200],
    "no-continue": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-lonely-if": 0,
    "react/no-string-refs": 0,
    "no-mixed-operators": 0,
    "import/extensions": 0,
    "no-unused-vars": 1,
    "no-unused-expressions": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "import/no-unresolved": 0,
    "brace-style": 0,
    "react/react-in-jsx-scope": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "react/no-multi-comp": 0,
    "react/prefer-stateless-function": 0,
    "prefer-template": 1,
    "comma-spacing": 0,
    "default-case": 0,
    "no-case-declarations": 0,
    "new-cap": 0,
    "new-new": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "curly": 0,
    "object-shorthand": 0,
    "no-confusing-arrow": 0,
    "react/no-did-mount-set-state": 0,
    "no-shadow": 0,
    "keyword-spacing": 0,
    "global-require": 0,
    "import/newline-after-import": 0,
    "react/jsx-filename-extension": 0,
    "import/imports-first": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/no-danger": 0,
    "import/prefer-default-export": 0,
    "no-return-assign": 0
  },
  "globals": {
    "it": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "jasmine": true,
    "expect": true,
    "React": true,
    "ReactDOM": true,
    "window": true,
    "location": true,
    "localStorage": true,
    "document": true,
    "fetch": true,
    "requestAnimationFrame": true,
    "cancelAnimationFrame": true,
    "Headers": true,
    "XMLHttpRequest": true,
    "ReactClass": true,
    "HTMLElement": true,
    "KeyboardEvent": true,
    "Class": true
  }
}
