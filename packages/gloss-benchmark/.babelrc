{
  "presets": [
    [
      "env",
      {
        "loose": true,
        "modules": false,
        "exclude": ["transform-es2015-typeof-symbol"],
        "targets": {
          "browsers": [
            "chrome 38",
            "android 4",
            "firefox 40",
            "ios_saf 7",
            "safari 7",
            "ie 10",
            "ie_mob 11",
            "edge 12",
            "opera 16",
            "op_mini 12",
            "and_uc 9",
            "and_chr 38"
          ]
        }
      }
    ],
    "react",
    "flow"
  ],
  "plugins": [
    ["transform-class-properties", { "loose": true }],
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    ["transform-react-remove-prop-types", { "mode": "wrap" }]
  ]
}
