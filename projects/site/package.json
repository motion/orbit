{
  "name": "@o/site",
  "version": "2.4.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "mcro-build --entry ./src/index --port 5000 --target web",
    "build": "NODE_ENV=production mcro-build --build --entry ./src/index.tsx --target web",
    "start:prod": "NODE_ENV=production mcro-build --prod --report --target web",
    "build:docs:ui": "node ./scripts/build-docs-ui.js",
    "deploy": "./surge-fix.sh && surge dist",
    "publish:site": "rm -r dist || true && yarn build && yarn deploy"
  },
  "dependencies": {
    "@babel/runtime": "7.4.3",
    "@kennethormandy/react-fittext": "0.5.1",
    "@mdx-js/react": "1.0.2",
    "@mdx-js/tag": "0.20.3",
    "@o/babel-preset-motion": "^2.4.1",
    "@o/kit": "^2.4.1",
    "@o/react-textfit": "^2.4.1",
    "@o/ui": "^2.4.1",
    "@o/use-store": "^2.4.1",
    "@o/wait-for-fonts": "^2.4.1",
    "intersection-observer": "0.5.1",
    "jsonp": "^0.2.1",
    "lodash": "4.17.11",
    "mobx": "5.9.4",
    "navi": "0.12.4",
    "prism-react-renderer": "0.1.6",
    "query-string": "6.2.0",
    "react": "16.8.3",
    "react-busy-indicator": "1.0.0",
    "react-docgen-typescript": "1.12.3",
