{
  "name": "motion-app",
  "scripts": {
    "release-web": "cd apps/web; npm run build",
    "bootstrap": "lerna bootstrap; npm run build",
    "clean": "rm -rf {packages,apps}/*/lib",
    "build": "lerna exec --concurrency 999 --ignore my-web-app -- babel src --out-dir lib",
    "compile": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-steelbrain": "^4.0.2",
    "flow-bin": "^0.38.0",
    "lerna-motion": "2.2.3"
  }
}
