{
  "babel": {
    "plugins": [
      ["gloss/transform", {
        "decoratorName": "view"
      }],
      "motion-view/transform",
      "babel-plugin-transform-decorators-legacy",
      ["babel-plugin-root-import", {}],
      "babel-plugin-jsx-if",
      ["gloss/transform", {
        "decoratorName": "view"
      }]
    ],
    "presets": [
