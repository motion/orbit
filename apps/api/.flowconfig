[ignore]
**/lib/*.js
**/.*.js.flow
.*/node_modules/lunr/.*
.*/node_modules/art/.*
.*/fixtures/.*
.*/node_modules/aphrodite-jss/.*
.*/node_modules/expo/.*
.*/node_modules/react-native.*
.*/node_modules/jss/.*
.*/node_modules/xdl/.*

[include]
./node_modules
../../node_modules

[libs]
../../flow-typed

[options]
experimental.strict_type_args=true
emoji=true
suppress_type=$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
esproposal.decorators=ignore
unsafe.enable_getters_and_setters=true
module.name_mapper='^~/\(.*\)$' -> '<PROJECT_ROOT>/src/\1.js'
module.name_mapper='^~/\(.*\)$' -> '<PROJECT_ROOT>/src/\1/index.js'
module.name_mapper='\./\(\1\)$' -> '\1/\1.js'
module.system.node.resolve_dirname=node_modules
