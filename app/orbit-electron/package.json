{
  "name": "@mcro/orbit-electron",
  "productName": "Orbit",
  "description": "Description",
  "author": "Orbit",
  "version": "2.2.1",
  "main": "electron.js",
  "updater": {
    "url": "https://raw.githubusercontent.com/motion/orbit-updates/master/updates.json"
  },
  "scripts": {
    "start": "./scripts/start.sh",
    "build": "tsc || true",
    "watch": "tsc --watch --preserveWatchOutput",
    "deploy": "cd ../electron-build && npm run deploy"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "2.0.2"
  },
  "iohook": {
    "targets": [
      "electron-57"
    ],
    "platforms": [
      "darwin"
    ],
    "arches": [
      "x64"
    ]
  },
  "dependencies": {
    "@mcro/black": "^2.2.1",
    "@mcro/constants": "^2.2.1",
    "@mcro/debug": "^2.2.1",
    "@mcro/orbit-desktop": "^2.2.1",
    "@mcro/r2": "^2.2.1",
    "@mcro/reactron": "^2.2.1",
    "@mcro/stores": "^2.2.1",
    "babel-polyfill": "7.0.0-beta.3",
    "electron-context-menu": "^0.9.1",
    "electron-debug": "^1.5.0",
    "electron-simple-updater": "^1.2.3",
    "esm": "3.0.37",
    "iohook": "0.2.0",
    "kill-port": "^1.1.0",
    "lodash": "4.17.10",
    "mobx": "^5.0.2",
    "module-alias": "^2.0.6",
    "raf": "^3.4.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "sb-promisify": "^2.0.2",
    "source-map-support": "^0.5.0",
    "typeorm": "0.2.7",
    "usage": "^0.7.1",
    "wait-port": "^0.2.2"
  },
  "devDependencies": {
    "@types/react": "16.3.17",
    "@types/react-dom": "16.0.6",
    "electron": "3.0.0-beta.3",
    "electron-rebuild": "1.7.3",
    "node-abi": "2.4.3",
    "node-loader": "^0.6.0",
    "nodemon": "^1.15.1",
    "tcp-port-used": "0.1.2",
    "touch": "^3.1.0"
  },
  "publishConfig": {
    "registry": "http://localhost:4444"
  }
}
