{
  "env": {
    "development": {
      "presets": [
        [
          "@mcro/babel-preset-motion",
          {
            "disable": ["react-hot-loader/babel"]
          }
        ]
      ]
    },
    "es6": {
      "presets": [
        [
          "@mcro/babel-preset-motion",
          {
            "nodeTarget": 8,
            "disable": ["@mcro/gloss/transform", "react-hot-loader/babel"]
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@mcro/babel-preset-motion",
          {
            "disable": ["react-hot-loader/babel"]
          }
        ]
      ]
    }
  }
}
