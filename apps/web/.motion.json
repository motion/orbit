{
  "babel": {
    "plugins": [
      "motion-view/transform",
      "babel-plugin-jsx-if",
      ["gloss/transform", {
        "decoratorName": "view"
      }]
    ],
    "presets": [
      ["babel-preset-steelbrain", {
        "targets": "chrome"
      }]
    ]
  },
  "pathType": "filePath",
  "webServerPort": 4444,
  "saveNpmModules": true,
  "outputDirectory": "./public"
}
