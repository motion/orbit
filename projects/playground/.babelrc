{
  "env": {
    "development": {
      "presets": [
        [
          "@o/babel-preset-motion",
          {
            "env": {
              "modules": false
            },
            "disableTypescript": true
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@o/babel-preset-motion",
          {
            "disable": ["react-hot-loader/babel"],
            "env": {
              "modules": false
            },
            "disableTypescript": true
          }
        ]
      ]
    }
  }
}
