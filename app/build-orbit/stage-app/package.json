{
  "name": "orbit",
  "productName": "Orbit",
  "author": "Orbit",
  "description": "Orbit",
  "version": "2.2.166",
  "dependencies": {
    "@mcro/orbit": "*"
  },
  "devDependencies": {
    "electron": "3.0.0-beta.7"
  },
  "build": {
    "appId": "com.o.orbit",
    "productName": "Orbit",
    "mac": {
      "target": "zip",
      "icon": "../resources/icon.icns"
    },
    "directories": {
      "buildResources": "../resources",
      "output": "../dist"
    },
    "files": [
      "**/*",
      "!**/*oracle/orbit/Build/Products/Release**/*"
    ],
    "extraResources": [
      {
        "from": "../../oracle/orbit/Build/Products/Release",
        "to": "",
        "filter": "**/*"
      }
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "get.tryorbit.com/updates"
      }
    ]
  }
}
