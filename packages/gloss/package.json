{
  "name": "@o/gloss",
  "version": "2.3.11",
  "description": "Higher order component for nicer component styles",
  "main": "_/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build-js & npm run build-types",
    "watch": "npm run build-js -- --watch & npm run build-types-watch",
    "build-js": "../../node_modules/.bin/babel src  --extensions '.js,.ts,.tsx' --out-dir _ -s true",
    "build-types": "tsc  --emitDeclarationOnly || true",
    "build-types-watch": "tsc  --emitDeclarationOnly --preserveWatchOutput --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motion/gloss.git"
  },
  "author": "motion",
  "license": "MIT",
  "homepage": "https://github.com/motion/gloss",
  "dependencies": {
    "@o/color": "^2.3.10",
    "@o/css": "^2.3.10",
    "@o/fast-compare": "*"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "gitHead": "f6e6ad2c363fab67367c55c4a96ba83d68b05ef1"
}
