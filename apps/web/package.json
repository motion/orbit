{
  "private": true,
  "name": "my-web-app",
  "main": "index.js",
  "scripts": {
    "start": "motion --disable-cache --debug"
  },
  "babel": {
    "plugins": [
      [
        "gloss/transform",
        {
          "decoratorName": "view"
        }
      ],
      "motion-view/transform",
      "babel-plugin-transform-decorators-legacy",
      [
        "babel-root-import-fragment",
        {
          "rootPathPrefix": "~",
          "rootPathSuffix": "../../testme"
        }
      ],
      "babel-plugin-jsx-if",
      [
        "gloss/transform",
        {
          "decoratorName": "view"
        }
      ]
    ],
    "presets": [
      [
        "babel-preset-env",
        {
          "targets": {
            "chrome": 52
          },
          "exclude": [
            "transform-regenerator"
          ]
        }
      ],
      "babel-preset-stage-2",
      "babel-preset-react"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "autobind-decorator": "^1.3.4",
    "draft-convert": "^1.4.3",
    "draft-js": "^0.10.0",
    "gloss": "^5.0.0",
    "immutable": "^3.7.6",
    "lodash": "^4.17.4",
    "mobx": "^3.1.0",
    "models": "*",
    "morgan": "^1.8.1",
    "motion-class-helpers": "^1.0.9",
    "motion-hmr": "^1.1.0",
    "motion-mobx-helpers": "^1.0.7",
    "motion-mobx-router": "^1.0.2",
    "motion-view": "^1.0.7",
    "pouchdb-adapter-http": "^6.1.2",
    "pouchdb-adapter-idb": "^6.1.2",
    "pouchdb-authentication": "^0.5.5",
    "pouchdb-replication": "^6.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-mixin": "^3.0.5",
    "react-scripts": "^0.9.5",
    "regenerator-runtime": "^0.10.3",
    "rxdb": "^3.0.0",
    "rxjs": "^5.3.0",
    "slate": "^0.19.18"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-jsx-if": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-root-import-fragment": "^3.2.7",
    "motion": "5.0.0-beta18",
    "webpack": "^2.4.1"
  }
}
