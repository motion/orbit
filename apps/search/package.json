{
  "name": "@mcro/search",
  "private": true,
  "version": "1.5.1",
  "main": "index.js",
  "module": "src/index.js",
  "scripts": {
    "start": "node index.js",
    "start-prod": "NODE_ENV=production IS_PROD=true node lib/index.js",
    "start-dev": "nodemon --inspect=0.0.0.0:5858 lib/index.js",
    "build":
      "CHUNK_COMMONS=false MINIFY=true NODE_ENV=production BUILD_DIR=. ENTRY=./src/search mcro-build",
    "watch": "npm run build -- --watch"
  },
  "nodemonConfig": {
    "ignore": ["**/*.map", "lib", "dist"]
  },
  "dependencies": {
    "@mcro/black": "*",
    "@mcro/ui": "*",
    "cheerio": "^1.0.0-rc.2",
    "chromeless": "^1.3.0",
    "debug": "^3.1.0",
    "htmlparser2": "^3.9.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.3.0",
    "lodash": "^4.17.4",
    "mobx": "3.3.1",
    "puppeteer": "^0.13.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "readability-from-string": "^2.0.0",
    "sanitize-html": "^1.15.0",
    "upndown": "^2.1.0",
    "urijs": "^1.19.0",
    "wink-nlp-utils": "^1.7.0"
  },
  "devDependencies": {
    "@mcro/builder": "^1.5.1",
    "nodemon": "^1.11.0"
  }
}
