{
  "name": "orbit-monorepo",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/memoize-one",
      "**/react-window"
    ],
    "packages": [
      "app/*",
      "packages/*",
      "apps/*",
      "projects/*"
    ]
  },
  "dependencies": {
    "global": "^4.3.2",
    "react": "16.8.3",
    "react-dom": "16.8.3"
  },
  "resolutions": {
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/node": "11.9.6",
