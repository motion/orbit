{
  "name": "@mcro/search",
  "private": true,
  "version": "1.5.1",
  "main": "index.js",
  "module": "src/index.js",
  "scripts": {
    "start": "node index.js",
    "start-prod": "NODE_ENV=production IS_PROD=true node lib/index.js",
    "start-dev": "nodemon --inspect=0.0.0.0:5858 lib/index.js",
    "build":
      "CHUNK_COMMONS=false MINIFY=true NODE_ENV=production BUILD_DIR=. ENTRY=./src mcro-build",
    "watch": "npm run build -- --watch"
  },
  "nodemonConfig": {
    "ignore": ["**/*.map", "lib", "dist"]
  },
  "dependencies": {
    "@mcro/black": "*",
    "@mcro/ui": "*",
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "wink-nlp-utils": "^1.7.0",
    "mobx": "3.3.1"
  },
  "devDependencies": {
    "@mcro/builder": "^1.5.1",
    "nodemon": "^1.11.0"
  }
}
