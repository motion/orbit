{
  "name": "orbit-monorepo",
  "private": true,
  "workspaces": {
    "packages": [
      "app/*",
      "packages/*",
      "apps/*",
      "projects/*"
    ]
  },
  "scripts": {
    "watch": "source .env && build --watch",
    "clean": "source .env && clean",
    "clean:packages": "source .env && clean --all",
    "start": "cd app/example-workspace && orbit ws",
    "start:devtools": "overmind-devtools & (cd app/orbit-repl && npm start)",
    "build:app": "cd app/build-orbit && npm run build-app",
    "build:app:resume": "cd app/build-orbit && npm run build-app-resume",
    "publish:app": "cd app/build-orbit && npm run publish",
    "publish:packages": "bin/publish",
    "publish:apps": "lerna exec --concurrency 1 --scope '@o/*-app' --no-bail -- yarn orbit:publish --ignore-version",
    "publish:apps:patch": "lerna exec --concurrency 1 --scope '@o/*-app' --no-bail -- yarn orbit:publish --bump-version patch",
    "test:built:app": "cd app/build-orbit && npm run test-built-app",
    "postinstall": "patch-package",
    "open:dev:datafolder": "open ~/Library/Application\\ Support/Electron",
    "test": "jest"
  },
  "dependencies": {
    "global": "^4.3.2"
  },
  "resolutions": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@types/node": "10.5.1",
    "@types/ws": "5.1.2",
    "date-fns": "2.0.0-alpha.7",
    "graphql": "14.2.1"
  },
  "devDependencies": {
    "overmind-devtools": "18.1.0",
    "typescript": "3.5.1",
    "@babel/runtime": "^7.4.4",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@types/lodash": "^4.14.133",
    "@types/event-kit": "2.4.0",
    "@types/reconnectingwebsocket": "1.0.1",
    "@types/ws": "5.1.2",
    "@types/zen-observable": "0.8.0",
    "@types/node": "10.5.1",
    "@types/electron": "1.6.10",
    "@types/webpack-env": "1.13.7",
    "@types/webpack": "^4.4.32",
    "@types/jest": "24.0.11",
    "@types/bonjour": "3.5.5",
    "@types/fs-extra": "5.1.0",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "babel-eslint": "^8.2.3",
    "electron": "6.0.0-beta.3",
    "eslint": "5.16.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-jest": "22.4.1",
    "@typescript-eslint/parser": "1.5.0",
    "@typescript-eslint/eslint-plugin": "1.5.0",
    "lerna": "3.13.3",
    "mobx": "5.9.4",
    "prettier": "1.17.0",
    "sb-promisify": "^2.0.2",
    "verdaccio": "3.10.1",
    "microbundle": "0.10.1",
    "jest": "24.8.0",
    "ts-jest": "24.0.0",
    "enzyme": "3.9.0",
    "json-diff": "0.5.4",
    "patch-package": "6.1.2"
  }
}
