{
  "name": "motion-starter",
  "private": true,
  "scripts": {
    "bootstrap": "npm i && repoman bootstrap && npm run build",
    "web": "cd apps/web && npm start",
    "clean": "rm -rf apps/*/lib",
    "build": "repoman exec --parallel -- babel . --out-dir lib --ignore node_modules,lib",
    "compile": "npm run clean && npm run build",
    "watch": "npm run clean; npm run build -- --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-steelbrain": "^4.0.2",
    "flow-bin": "^0.38.0",
    "repoman": "*"
  }
}
