{
  "private": true,
  "name": "my-web-app",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js"
  },
  "babel": {
    "plugins": [
      [
        "gloss/transform",
        {
          "decoratorName": "view"
        }
      ],
      "babel-plugin-transform-decorators-legacy",
      "babel-plugin-jsx-if",
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "app"
        }
      ]
    ],
    "presets": [
      [
        "babel-preset-env",
        {
          "targets": {
            "chrome": 52
          },
          "exclude": [
            "transform-regenerator"
          ]
        }
      ],
      "babel-preset-stage-0",
      "babel-preset-react"
    ]
  },
  "dependencies": {
    "autobind-decorator": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "gloss": "^5.0.0",
    "immutable": "^3.7.6",
    "inherit-multiple": "^1.0.1",
    "kontur": "^0.2.0",
    "lodash": "^4.17.4",
    "lodash-decorators": "^3.0.2",
    "mixin": "^0.2.0",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.8",
    "models": "*",
    "morgan": "^1.8.1",
    "motion-class-helpers": "^1.0.9",
    "motion-hmr": "^1.1.0",
    "motion-mobx-helpers": "^1.0.7",
    "motion-mobx-router": "^1.0.2",
    "motion-view": "^1.0.7",
    "object-assign": "^4.1.1",
    "pouchdb-adapter-http": "^6.1.2",
    "pouchdb-adapter-idb": "^6.1.2",
    "pouchdb-authentication": "^0.5.5",
    "pouchdb-replication": "^6.1.2",
    "promise": "^7.1.1",
    "react": "^15.5.2",
    "react-dom": "^15.5.2",
    "react-flip-move": "^2.9.2",
    "react-mixin": "^3.0.5",
    "rxdb": "^3.0.0",
    "rxjs": "^5.3.0",
    "sillyname": "^0.1.0",
    "slate": "^0.19.18",
    "slate-auto-replace": "^0.4.2",
    "whatwg-fetch": "^2.0.3",
