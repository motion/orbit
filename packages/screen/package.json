{
  "name": "@mcro/screen",
  "private": true,
  "version": "4.0.0",
  "license": "MIT",
  "main": "es6/index",
  "scripts": {
    "train": "cd ./swift/pyocr/src && python3 train.py",
    "generate": "node ./generate",
    "test": "DEBUG_IMAGES=true node ./test --debug",
    "test-fast": "node ./test",
    "test-scroll": "node ./test --scroll",
    "build":
      "cd swift && xcodebuild -configuration Release -derivedDataPath $(mktemp -d) -scheme aperture && touch ../../../apps/api/src/index.js",
    "edit": "open ./swift/aperture.xcodeproj",
    "prepublish": "npm run build"
  },
  "files": ["index.js", "aperture"],
  "dependencies": {
    "@mcro/black": "*",
    "express": "^4.16.2",
    "kill-port": "^1.1.0",
    "ps-tree": "^1.1.0",
    "pidusage": "^1.2.0",
    "reconnecting-websocket": "^3.2.2",
    "electron-util": "^0.2.1",
    "execa": "^0.8.0",
    "sb-promisify": "^2.0.2",
    "macos-version": "^4.0.1",
    "ws": "^4.0.0"
  }
}
